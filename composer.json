{
    "name": "ubw/dativ",
    "description": "Archive Management System",
    "type": "project",
    "version": "1.9.4",
    "license": "BSD-3-Clause",
    "keywords": [
        "laminas",
        "mvc",
        "skeleton",
        "framework"
    ],
    "require": {
        "php": ">= 8.2",
        "ext-zip": "*",
        "ext-intl": "*",
        "ext-imagick": "*",
        "ext-pdo_mysql": "*",
        "ext-ldap": "*",
        "laminas/laminas-mvc": "^3.1.1",
        "laminas/laminas-log": "^2.13.1",
        "laminas/laminas-mvc-i18n": "^1.2.0",
        "laminas/laminas-json": "^3.2",
        "laminas/laminas-permissions-acl": "^2.8",
        "laminas/laminas-authentication": "^2.8",
        "laminas/laminas-session": "^2.10",
        "laminas/laminas-http": "^2.14",
        "laminas/laminas-navigation": "^2.11",
        "laminas/laminas-form": "^3.0",
        "laminas/laminas-code": "^4.4",
        "laminas/laminas-i18n": "^2.11",
        "laminas/laminas-view": "^2.12",
        "laminas/laminas-paginator": "^2.10",
        "laminas/laminas-ldap": "^2.11",
        "laminas/laminas-xml2json": "^3.2",
        "laminas/laminas-cli": "^1.4",
        "laminas/laminas-mvc-plugin-flashmessenger": "^1.7",
        "laminas/laminas-mail": "^2.16",
        "laminas/laminas-mime": "^2.12",
        "doctrine/migrations": "^3.3",
        "doctrine/doctrine-orm-module": "^5.1",
        "jfcherng/php-diff": "^6.15",
        "tecnickcom/tcpdf": "^6.7",
        "goat1000/svggraph": "^3.20",
        "beberlei/doctrineextensions": "^1.5"
    },
    "require-dev": {
        "ext-xdebug": "*",
        "laminas/laminas-developer-tools": "^2.1.1",
        "san/san-session-toolbar": "^4.0",
        "php-parallel-lint/php-parallel-lint": "^1.3",
        "dealerdirect/phpcodesniffer-composer-installer": "^0.7.2",
        "sirbrillig/phpcs-variable-analysis": "^2.11",
        "squizlabs/php_codesniffer": "^3.7",
        "slevomat/coding-standard": "^8.6",
        "phly/keep-a-changelog": "^2.12",
        "phpstan/phpstan": "^1.10",
        "phpstan/extension-installer": "^1.3",
        "slam/phpstan-laminas-framework": "^1.3",
        "rector/rector": "^1.2",
        "tomasvotruba/type-coverage": "^1.0",
        "vimeo/psalm": "^6.5",
        "psalm/plugin-phpunit": "^0.19.3",
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/",
            "Opus\\": "module/Opus/src/",
            "File\\": "module/File/src/",
            "Tag\\": "module/Tag/src/",
            "Api\\": "module/Api/src/",
            "AccessControl\\": "module/AccessControl/src/",
            "Worker\\": "module/Worker/src",
            "Dativ\\": "module/Dativ/src",
            "PublicDownload\\": "module/PublicDownload/src",
            "ActionLog\\": "module/ActionLog/src",
            "AppSetting\\": "module/AppSetting/src",
            "Healthcheck\\": "module/Healthcheck/src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ApplicationTest\\": "module/Application/test/",
            "OpusTest\\": "module/Opus/test/"
        }
    },
    "scripts": {
        "cs-check": "phpcs -s",
        "cs-fix": "phpcbf",
        "plint": "parallel-lint",
        "development-disable": "laminas-development-mode disable",
        "development-enable": "laminas-development-mode enable",
        "development-status": "laminas-development-mode status",
        "post-create-project-cmd": [
            "@development-enable",
            "php bin/update-gitignore.php",
            "php -r 'if (file_exists(\"bin/remove-package-artifacts.php\")) include \"bin/remove-package-artifacts.php\";'",
            "php -r 'if (file_exists(\"CHANGELOG.md\")) unlink(\"CHANGELOG.md\");'"
        ],
        "post-install-cmd": [
            "cp log/archivedWuesyphusOpus.log.dist log/archivedWuesyphusOpus.log"
        ],
        "serve": "php -S 0.0.0.0:8080 -t public",
        "test": "phpunit",
        "dmdl": "doctrine-module",
        "kca": "keep-a-changelog",
        "phpstan": "phpstan",
        "static-analysis": "vendor/bin/psalm --stats --clear-cache",
        "sa": "@static-analysis",
        "qa": [
            "@plint module/",
            "@cs-check",
            "@phpstan",
            "@sa"
        ]
    },
    "scripts-descriptions": {
        "cs-check": "Run coding standards checks.",
        "cs-fix": "Automatically fix coding standard issues.",
        "development-disable": "Disable development mode.",
        "development-enable": "Enable development mode.",
        "development-status": "Detail whether or not the application is in development mode.",
        "serve": "Start the built-in PHP web server and serve the application.",
        "test": "Run unit tests.",
        "kca": "phly/keep-a-changelog Changelog Management.",
        "phpstan": "Static analysis through phpstan"
    },
    "config": {
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "php-http/discovery": false,
            "phpstan/extension-installer": true,
            "helhum/dotenv-connector": false
        }
    }
}

